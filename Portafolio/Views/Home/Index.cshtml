@model Persona

@{
    ViewData["Title"] = "Home Page";
    DateTime hoy = DateTime.Now;
}

<div class="text-center">

    <h1 class="display-4">Soy @Model.Nombre (@Model.Edad)</h1>

    <h4>@hoy.ToString("dd/MM/yyyyy hh:mm:ss tt")</h4>    

    <h2>Soy un desarrollador full stack especializado en tecnologías .NET y JavaScript</h2>


    <img id="foto" src="~/imagenes/jimmyg.jpeg" />
</div>
